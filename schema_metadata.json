        "description": "Weight of the product in grams"
    },
    {
        "table": "olist_products",
        "column": "product_length_cm",
        "type": "DECIMAL",
        "description": "Length of the product in centimeters"
    },
    {
        "table": "olist_products",
        "column": "product_height_cm",
        "type": "DECIMAL",
        "description": "Height of the product in centimeters"
    },
    {
        "table": "olist_products",
        "column": "product_width_cm",
        "type": "DECIMAL",
        "description": "Width of the product in centimeters"
    },
    {
        "table": "olist_sellers",
        "column": "seller_id",
        "type": "VARCHAR",
        "description": "Unique identifier for each seller"
    },
    {
        "table": "olist_sellers",
        "column": "seller_zip_code_prefix",
        "type": "INT",
        "description": "ZIP/postal code prefix of the seller’s location"
    },
    {
        "table": "olist_sellers",
        "column": "seller_city",
        "type": "VARCHAR",
        "description": "City where the seller is located"
    },
    {
        "table": "olist_sellers",
        "column": "seller_state",
        "type": "CHAR",
        "description": "State where the seller is located"
    }
]
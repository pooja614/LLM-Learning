    },
    {
        "table": "olist_products",
        "column": "product_name_lenght",
        "type": "INT",
        "description": "Length of the product name text"
    },
    {
        "table": "olist_products",
        "column": "product_description_lenght",
        "type": "INT",
        "description": "Length of the product description text"
    },
    {
        "table": "olist_products",
        "column": "product_photos_qty",
